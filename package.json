{
  "name": "gainz-deck",
  "version": "0.14.3",
  "main": "expo-router/entry",
  "private": true,
  "author": "dendrovis team",
  "scripts": {
    "dev": "expo start --android",
    "checks": "npx expo-doctor",
    "test:linter": "eslint . --fix-dry-run && prettier . --check",
    "test:unit": "echo wip",
    "test:e2e": "echo wip",
    "prebuild": "eas whoami && expo prebuild --platform android",
    "build:local": "eas build --profile preview --platform android --local --output build/app.apk",
    "build:local-raw": "cd android && gradlew build && cd .."
  },
  "dependencies": {
    "expo": "~51.0.38",
    "expo-constants": "~16.0.2",
    "expo-image": "~1.13.0",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.10.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "~18.2.79",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.2",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.4.0"
  }
}
